@model KillerAppASP.ViewModels.MapListViewModel

<script>
    window.onload = function () {
        GetUserMaps();
    }
</script>

<div id="MapList" class="list-group">
    @if (Model != null && Model.Maps.Count != 0)
    {
        <div class="pre-scrollable">
            @foreach (string map in Model.Maps)
            {
                <button class="list-group-item" id="@map" type="button" onclick="GetPreview('@map')">@map</button>
            }
        </div>
    }
    else
    {
        <a class="list-group-item list-group-item-danger">You don't have any maps yet.</a>
    }
</div>
