@model KillerAppASP.ViewModels.MapListViewModel

<script>
    window.onload = function () {
        GetUserMaps();
    }
</script>

<div id="MapList" class="list-group">
    <div class="pre-scrollable">
        @if (Model != null && Model.Maps.Count != 0)
        {
            foreach (string map in Model.Maps)
            {
                <button type="button" class="list-group-item" onclick="GetPreview('@map')">@map</button>
            }
        }
        else
        {
            <p class="text-danger">You don't have any maps yet.</p>
        }
    </div>
</div>