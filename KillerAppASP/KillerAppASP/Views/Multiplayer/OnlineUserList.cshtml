@model MultiplayerViewModel

<div class="list-group">
    <div class="pre-scrollable">
        @if (Model.userModel != null && Model.userModel.Users.Count != 0)
        {
            foreach (User user in Model.userModel.Users)
            {
                if (user.Username != User.Identity.Name && user.IsOnline == true)
                {
                    <a id='@user.UserID' class="list-group-item">@user.Username</a>
                }
            }
        }
    </div>
</div>